  <data name="AcatualizarVehiculo" xml:space="preserve">
    <value>update Vehiculo set 
Chasis =@chasis
      ,Placa =@Placa
      ,Transmision =@Transmision
      ,Combustible =@Combustible
      ,Mantenimiento =@Mantenimiento
      ,Anio =@anio
      ,ModeloID  =@ModeloID
      ,ColorID  =@ColorID
      ,EmpleadoID  =@EmpleadoID

	  where id= @id</value>
  </data>
