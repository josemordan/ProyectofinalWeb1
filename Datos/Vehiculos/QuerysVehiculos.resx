  <data name="ConsultaColores" xml:space="preserve">
    <value>SELECT  ID
      ,Nombre
  FROM dbo.Color where  borrado =0</value>
  </data>
  <data name="ConsultarMarca" xml:space="preserve">
    <value>SELECT  ID 
      , Nombre 
  FROM  dbo . Marca where borrado =0</value>
  </data>
  <data name="ConsultarModelos" xml:space="preserve">
    <value>SELECT a.ID
      ,a.Nombre
      ,MarcaID
	  ,b.Nombre as Marca
  FROM dbo.Modelo a
  inner join dbo.marca b on a.marcaid = b.id
    where a.borrado =0</value>
  </data>
  <data name="ConsultarVehiculo" xml:space="preserve">
    <value>
SELECT a.ID
      ,Chasis
      ,Placa
      ,Transmision
      ,Combustible
      ,Mantenimiento
      ,Anio
      ,ModeloID
      ,ColorID
      ,EmpleadoID
	  ,c.Nombre as color
	  ,m.Nombre as modelo
	  ,e.Nombre as empleado
  FROM dbo.Vehiculo a 
  inner join Color c on a.colorid =c.ID
  inner join Modelo m on a.modeloid =m.ID
  inner join Empleado e on a.EmpleadoID =e.ID
  where a.borrado =0 </value>
  </data>
